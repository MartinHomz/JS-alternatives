    private void BubbleSort(int[] a)
    {

        for (int i = 1; i <= a.Length - 1; ++i)
        {
            for (int j = 0; j < a.Length - i; ++j)
            {
                if (a[j] > a[j + 1])
                {
                    int temp = a[j];
                    a[j] = a[j + 1];
                    a[j + 1] = temp;
                }
            }
        }
    }

